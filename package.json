{
  "name": "priority-date",
  "version": "0.0.0-semantically-released",
  "description": "Greencard Priority Date checker",
  "main": "dist/index.js",
  "dependencies": {
    "generator-node": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.7.2",
    "babel-preset-es2015": "6.6.0",
    "bluebird": "3.3.4",
    "chai": "3.5.0",
    "chai-as-promised": "5.2.0",
    "cheerio": "^0.20.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.7.2",
    "cz-conventional-changelog": "1.1.5",
    "es6-promise": "3.1.2",
    "ghooks": "1.0.3",
    "isparta": "4.0.0",
    "istanbul": "1.0.0-alpha",
    "jsdom": "8.1.0",
    "mocha": "2.4.5",
    "request": "^2.69.0"
  },
  "czConfig": {
    "path": "node_modules/ca-conventional-changelog"
  },
	"config": {
		"ghooks": {
			"pre-commit": "npm run test:single && npm run check-coverage"
		}
	},
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 50 --branches 50 --functions 50 --lines 50",
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "babel src/index.js -o dist/index.js",
    "test:single": "babel-node ./node_modules/.bin/isparta cover --root src/ ./node_modules/.bin/_mocha -- -R spec src/index.test.js",
    "test": "mocha src/index.test.js -w --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shanshanyang/priority-date.git"
  },
  "keywords": [
    "greencard",
    "priority",
    "date",
    "scraper"
  ],
  "author": "Shanshan Yang <shanshanyang.ny@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shanshanyang/priority-date/issues"
  },
  "homepage": "https://github.com/shanshanyang/priority-date#readme"
}
